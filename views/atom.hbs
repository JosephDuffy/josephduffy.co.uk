<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Joseph Duffy's Blog</title>
	<link rel="alternate" type="text/html" href="https://josephduffy.co.uk/"/>
	<link rel="self" type="application/atom+xml" href="https://josephduffy.co.uk/atom.xml"/>
	<rights>Copyright Â© 2015, Joseph Duffy</rights>
	<updated>{{{formatDate lastUpdated "YYYY-MM-DDTHH:mm:ssZ"}}}</updated>
	<id>https://josephduffy.co.uk/atom.xml</id>

	{{#each posts}}
	<entry>
		<title>{{this.title}}</title>
		<link href="https://josephduffy.co.uk{{this.url}}" />
		<id>https://josephduffy.co.uk{{this.url}}</id>
		<published>{{{formatDate this.date "YYYY-MM-DDTHH:mm:ssZ"}}}</published>
		<updated>{{{formatDate this.date "YYYY-MM-DDTHH:mm:ssZ"}}}</updated>
		<author>
			<name>Joseph Duffy</name>
			<uri>https://josephduffy.co.uk/</uri>
		</author>
		<content type="html">
		<![CDATA[
		{{{this.preview}}}
		]]>
		</content>
	</entry>
	{{/each}}
</feed>